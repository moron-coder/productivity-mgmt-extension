{
    "manifest_version": 2,
    "name": "Timer app",
    "version": "1.0",
    "description": "Making a timer",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "storage",
        "notifications",
        "contextMenus",
        "tabs",
        "alarms"
    ],
    "icons": {
        "16": "img16.png",
        "48": "img48.png",
        "128": "img128.png"
    }
}